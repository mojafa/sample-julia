{
    "name": "Julia Development",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".."
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "julialang.language-julia"
            ],
            "settings": {
                "[julia]": {
                    "editor.defaultFormatter": "julialang.language-julia",
                    "editor.formatOnSave": true
                },
                "julia.execution.resultType": "inline"
            }
        }
    }
}